@using SponsorY.DataAccess.ModelsAccess;
@model IEnumerable<NotAcceptedTransactionViewModel>

@{
	ViewData["Title"] = "Requested";
}

<partial name="_TempData" />
<h2 class="col-md-4 offset-md-4">Requested sponsorship awaiting acceptance</h2>



<div class="text-center row">
	@foreach (var spon in Model)
	{
		<div class="card col-4" style="width: 20rem; ">
			
			<div class="card-body">
				<img src="data:image/png;base64,">
				<p class="mb-0">Number of clips requered: @spon.QuntityClips</p>
				<p class="mb-0">Total money: @spon.TransferMoveney $</p>
				<p class="mb-0">Chanel name: @spon.YoutubeChanelName</p>
				<p class="mb-0">Chanel subscribers: @spon.ChanelSubscribers</p>
				<div class="w-75 btn-group" role="group">
					<a class="btn btn-outline-primary" asp-area="Transaction" asp-controller="Transaction" asp-action="Edit" asp-route-TransId=@spon.Id><i class="bi bi-pencil-square"></i> Edit</a>
				</div>
				<div class="w-75 btn-group" role="group">
					<a class="btn btn-outline-primary" asp-area="Transaction" asp-controller="Transaction" asp-action="Delete" asp-route-TransId=@spon.Id><i class="bi bi-x-circle"></i> Delete</a>
				</div>

			</div>
		</div>
	}
</div>